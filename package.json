{
  "name": "bettingapp",
  "version": "1.0.0",
  "description": "This app allows people to view sporting games, place bets on the games, and accumulate credits.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:prod": "node server.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattsainson/bettingapp.git"
  },
  "keywords": [],
  "author": "Kyle, Lovdeep, Matt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattsainson/bettingapp/issues"
  },
  "homepage": "https://github.com/mattsainson/bettingapp#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chart.js": "^2.8.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "flexbox": "0.0.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "math.js": "^1.1.43",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "react-chartjs-2": "^2.7.6",
    "react-moment": "^0.9.2",
    "sequelize": "^5.8.6",
    "sequelize-cli": "^5.4.0",
    "unirest": "^0.6.0"
  }
}
